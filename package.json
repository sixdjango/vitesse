{
  "name": "django-vue3-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.30.0",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build --mode production && esno ./build/scripts/postBuild.ts production",
    "build:staging": "cross-env NODE_ENV=staging vite build --mode staging && esno ./build/scripts/postBuild.ts staging",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "axios": "^1.6.7",
    "element-plus": "^2.6.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify-json/carbon": "^1.1.16",
    "@intlify/unplugin-vue-i18n": "^0.9.2",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "@types/shelljs": "^0.8.11",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.3.1",
    "autoprefixer": "^10.4.18",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-plugin-cypress": "^2.12.1",
    "esno": "^4.0.0",
    "https-localhost": "^4.7.1",
    "less": "^4.2.0",
    "lint-staged": "^13.2.0",
    "pnpm": "^7.30.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "shelljs": "^0.8.5",
    "shiki": "^0.14.1",
    "simple-git-hooks": "^2.8.1",
    "swagger-typescript-api": "^12.0.3",
    "tailwindcss": "^3.4.1",
    "taze": "^0.13.3",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^5.1.4",
    "vite-plugin-externals": "^0.6.2",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-inspect": "^0.8.3",
    "vitest": "^0.29.3",
    "vue-tsc": "^1.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
